{
  "name": "domscript-pro",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "dev": "next-remote-watch ./src/content",
    "build": "next build && node --experimental-modules ./scripts/downloadFonts.mjs",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:source": "prettier --config .prettierrc --write \"{plugins,src}/**/*.{js,ts,jsx,tsx,css}\"",
    "nit:source": "prettier --config .prettierrc --list-different \"{plugins,src}/**/*.{js,ts,jsx,tsx,css}\"",
    "prettier": "yarn format:source",
    "prettier:diff": "yarn nit:source",
    "lint-heading-ids": "node scripts/headingIdLinter.mjs",
    "fix-headings": "node scripts/headingIdLinter.mjs --fix",
    "ci-check": "npm-run-all prettier:diff --parallel lint tsc lint-heading-ids",
    "tsc": "tsc --noEmit",
    "start": "next start",
    "check-all": "npm-run-all prettier lint:fix tsc"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "1.15.5",
    "@docsearch/css": "^3.3.3",
    "@docsearch/react": "^3.3.3",
    "@headlessui/react": "^1.7.14",
    "@reduxjs/toolkit": "^1.9.5",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "debounce": "^1.2.1",
    "ga-lite": "^2.1.4",
    "github-slugger": "^2.0.0",
    "next": "^13.3.0",
    "next-remote-watch": "^1.0.0",
    "parse-numeric-range": "^1.3.0",
    "react": "^18.2.0",
    "react-collapsed": "npm:@gaearon/react-collapsed@3.1.0-forked.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.32.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-react": "^7.18.6",
    "@mdx-js/mdx": "^2.3.0",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/classnames": "^2.2.10",
    "@types/debounce": "^1.2.1",
    "@types/mdx-js__react": "^1.5.5",
    "@types/node": "^18.15.11",
    "@types/parse-numeric-range": "^0.0.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "asyncro": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "babel-eslint": "10.x",
    "eslint": "7.x",
    "eslint-config-next": "^13.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^0.0.0-experimental-fabef7a6b-20221215",
    "fs-extra": "^11.1.1",
    "globby": "^13.1.4",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.1",
    "mdast-util-to-string": "^3.2.0",
    "metro-cache": "^0.76.2",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.5.1",
    "postcss": "^8.4.22",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^2.8.7",
    "reading-time": "^1.5.0",
    "remark": "^14.0.2",
    "remark-external-links": "^9.0.1",
    "remark-html": "^15.0.2",
    "remark-images": "^3.1.0",
    "remark-slug": "^7.0.1",
    "remark-unwrap-images": "^3.0.1",
    "retext": "^8.1.0",
    "retext-smartypants": "^5.2.0",
    "rss": "^1.2.2",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4",
    "unist-util-visit": "^4.1.2",
    "webpack-bundle-analyzer": "^4.8.0"
  },
  "engines": {
    "node": ">=16.x"
  },
  "nextBundleAnalysis": {
    "budget": null,
    "budgetPercentIncreaseRed": 10,
    "showDetails": true
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,css}": "yarn prettier",
    "src/**/*.md": "yarn fix-headings"
  }
}
